    "build:dev": "npm run clean-build && webpack --config webpack.config.dev.js",
    "build:dev:express": "npm run build:dev && sh localserver.sh",
    "start-express": "APP_NAME=kandidatsok node ./server.js",
    "start-express-veileder": "node ./server-veileder.js",
    "start-express-next": "APP_NAME=kandidatsok-next node ./server.js",
    "start": "webpack-dev-server --port 9009 --inline --env.dev --config webpack.config.dev.js",
    "start-veileder": "webpack-dev-server --port 9010 --inline --env.dev --config webpack.config.veileder.dev.js",
    "lint": "eslint \"./src/**/*.{js,jsx}\" --fix",
    "lint-staged": "bash ./scripts/pre-commit.sh"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "bash ./scripts/commit-msg.sh"
    }
