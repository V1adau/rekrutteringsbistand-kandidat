    "sauce-iphone_x": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env iphone_x",
    "sauce-galaxy_s9_plus_fhd": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env galaxy_s9_plus_fhd",
    "sauce-all": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env default,firefox,edge,ie,safari,iphone_x",
    "sauce-jenkins-chrome": "xvfb-run node_modules/.bin/nightwatch --config nightwatch-sl.conf.js",
    "sauce-jenkins-all": "xvfb-run node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env firefox,edge,ie,safari,iphone_x",
