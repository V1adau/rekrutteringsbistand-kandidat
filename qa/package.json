    "sauce-iphone_x": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env iphone_x",
    "sauce-galaxy_s9_plus_fhd": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env galaxy_s9_plus_fhd",
    "sauce-jenkins-default": "xvfb-run node_modules/.bin/nightwatch --config nightwatch-sl.conf.js",
