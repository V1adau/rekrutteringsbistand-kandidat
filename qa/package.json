    "sauce-chrome-windows": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env default,chrome-1_W10,chrome-2_W10,chrome_W8,chrome-1_W8,chrome-2_W8,chrome_W7,chrome-1_W7,chrome-2_W7",
