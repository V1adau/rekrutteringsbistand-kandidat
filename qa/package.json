    "chrome-jenkins": "xvfb-run node_modules/.bin/nightwatch --env jenkins",
    "sauce": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js",
    "sauce-chrome": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env default,chrome-1_W10,chrome-2_W10,chrome_W8,chrome-1_W8,chrome-2_W8,chrome_W7,chrome-1_W7,chrome-2_W7",
    "sauce-chrome-1_W10": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome-1_W10",
    "sauce-chrome-2_W10": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome-2_W10",
    "sauce-chrome_W8": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome_W8",
    "sauce-chrome-1_W8": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome-1_W8",
    "sauce-chrome-2_W8": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome-2_W8",
    "sauce-chrome_W7": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome_W7",
    "sauce-chrome-1_W7": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome-1_W7",
    "sauce-chrome-2_W7": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env chrome-2_W7",
    "sauce-edge": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env edge,edge-1,edge-2",
    "sauce-ie": "node_modules/.bin/nightwatch --config nightwatch-sl.conf.js --env ie_W10,ie_W8,ie_W7",
