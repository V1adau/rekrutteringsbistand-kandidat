{
  "name": "pam-kandidatsok-qa",
  "version": "1.0.0",
  "description": "Funksjonelle akseptansetester for PAM-kandidatsok",
  "scripts": {
    "chrome": "node_modules/.bin/nightwatch --config nightwatch.conf.js --env q6",
    "chrome-debug": "node --inspect node_modules/nightwatch/bin/nightwatch --config nightwatch.conf.js --env q6",
